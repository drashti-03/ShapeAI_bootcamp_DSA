#include <iostream>
using namespace std;

int main(){
   int i,j,k;
   for (int i = 5; i >= 1; i--)
   {
       for (int k = i ; k >=1 ; k--)
       {
          cout<<" ";
       }
       
       for (int j = 1 ; j <= 6-i ; j++)
       {
           cout<<"* ";
       }
       cout<<endl;
   }
    
return 0;
}
