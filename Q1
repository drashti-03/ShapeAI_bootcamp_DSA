#include<iostream>
using namespace std;
int a, b;

void swap_pointer(int* a, int* b){
int temp = *a;  
   *a = *b;       
   *b = temp;      
 }  
 
 int main(){
    
  cout<<"enter two numbers: "<<a<<b<<endl; 
